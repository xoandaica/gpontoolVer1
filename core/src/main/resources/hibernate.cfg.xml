<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<!-- The following doctype doesn't work when offline, so default to old one, which does work.
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">-->
<hibernate-configuration>
    <session-factory>
        <!--<property name="hibernate.dialect">org.hibernate.dialect.Oracle10gDialect</property>-->
        <!--<property name="format_sql">true</property>-->
        <property name="show_sql">false</property>
        <property name="hibernate.enable_lazy_load_no_trans">true</property>
        <property name="hibernate.hbm2ddl.auto">none</property>
        <property name="hibernate.connection.autocommit">true</property>
        
        <mapping class="vn.vnpttech.ssdc.nms.model.User"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.Role"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.ActionLogs"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.Area"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.Device"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.DeviceModel"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.Firmware"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.Policy"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.PolicyHistory"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.UpdateFirmwareLogs"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.AreaDeviceMapping"/>
        <mapping class="vn.vnpttech.ssdc.nms.model.ServiceLog"/>
    </session-factory>
</hibernate-configuration>
